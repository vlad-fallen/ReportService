{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "ReportService.Api": "Information"
    },
    "Console": {
      "FormatterOptions": {
        "SingleLine": true,
        "TimestampFormat": "HH:mm:ss ",
        "UseUtcTimestamp": true
      }
    }
  },
  "AllowedHosts": "*",
  "UseDeveloperExceptionPage": false,
  "UseSwagger": false
}
